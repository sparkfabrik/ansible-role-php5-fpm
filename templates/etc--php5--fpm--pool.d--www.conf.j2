[www]
user = www-data
group = www-data
listen = /var/run/php-fpm.sock
pm = dynamic
pm.max_children = {{ php5fpm_max_children }}
pm.start_servers = {{ php5fpm_start_servers }}
pm.min_spare_servers = {{ php5fpm_min_spare_servers }}
pm.max_spare_servers = {{ php5fpm_max_spare_servers }}
pm.max_requests = {{ php5fpm_max_requests }}
